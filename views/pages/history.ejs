<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/head'); %>
</head>

<body class="container">

  <header>
    <%- include('../partials/header'); %>
  </header>


  <div class="container" id="main">
    <div class="card border-0 shadow my-5">
      <div class="card-body p-5">
        <h1 class="fw-light">History</h1>

        <table class="table table-hover">
          <thead>
            <tr>
              <th scope="col">Code</th>
              <th scope="col">Name</th>
              <th scope="col">Ti·ªÅn</th>
              <th scope="col">Method</th>
              <th scope="col">Phone Number</th>
              <th scope="col">Message</th>
            </tr>
          </thead>
          <tbody>
            <% for (var i = 0; i < history.length; i++) { %>
            <tr menhgia-id="<%= history[i].menhgiaId %>" =>
              <th scope="row"><%= history[i].key %></th>
              <td><%= history[i].name%></td>
              <td><%= history[i].lixi%></td>
              <td><%= history[i].method%></td>
              <td><%= history[i].phoneNumber%></td>
              <td><%= history[i].message%></td>
            </tr>
            <% } %>
            
          </tbody>
        </table>
      </div>
    </div>
  </div>

  <footer>
    <%- include('../partials/footer'); %>
  </footer>
</body>

</html>